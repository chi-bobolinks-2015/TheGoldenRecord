<div class="container debug">
  <div class="nav nav-stacked fl-l col-lg-2 debug" id="sidebar">
    <ul>
      <% @categories.each do |category| %>
        <li class="category-name"><%= category.name %>
          <ul class="hidden">
            <% category.tracks.each do |track| %>
              <li class="draggableTrack" url="<%= track.url %>" id="<%= track.id %>" image="<%= category.img %>"><%= track.title %></li>
              <li><a href="#popup-box" class="popup-window">Learn More</a></li>
            <% end %>
          </ul>
        </li>
      <% end %>
    </ul>
  </div>

  <div id="popup-box" class="popup-info">
    <p>Things about music.</p>
    <button type="button" class="close">Close</button>
  </div>

  <div class="collapsible col-lg-1 debug"></div>
<div class="jquery-script-clear"></div>
  <div class="mix debug fl-l col-lg-9">
    <div class="track-divs honeycombs debug">

        <div id="1" class="cell comb border debug"><img src="https://upload.wikimedia.org/wikipedia/commons/7/7b/The_Sounds_of_Earth_-_GPN-2000-001976.jpg" /><span></span></div>
        <div id="2" class="cell comb border debug" ><img src="https://upload.wikimedia.org/wikipedia/commons/7/7b/The_Sounds_of_Earth_-_GPN-2000-001976.jpg" /><span></span></div>

        <div id="3" class="cell comb border debug"><img src="https://upload.wikimedia.org/wikipedia/commons/7/7b/The_Sounds_of_Earth_-_GPN-2000-001976.jpg" /><span></span></div>
        <div id="4" class="cell comb border debug"><img src="https://upload.wikimedia.org/wikipedia/commons/7/7b/The_Sounds_of_Earth_-_GPN-2000-001976.jpg" /><span></span></div>

        <div id="5" class="cell comb border debug"><img src="https://upload.wikimedia.org/wikipedia/commons/7/7b/The_Sounds_of_Earth_-_GPN-2000-001976.jpg" /><span></span></div>
        <div id="6" class="cell comb border debug"><img src="https://upload.wikimedia.org/wikipedia/commons/7/7b/The_Sounds_of_Earth_-_GPN-2000-001976.jpg" /><span></span></div>

    </div>

      <div class="deck debugs">
        Control Panel
          <p> Volume </p>
          <div id = "volume-slider" style="width:260px; margin:15px;" class="ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all">
            <div id= "volume-level" class="ui-slider-range ui-widget-header ui-corner-all ui-slider-range-min" style="left: 60%;"></div>
            <span id = "volume-button"class="ui-slider-handle ui-state-default ui-corner-all" tabindex="0" style="left: 60%;"></span>
          </div>
          <p> Pitch </p>
          <div id = "pitch-slider" style="width:260px; margin:15px;" class="ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all">
            <div id= "pitch-level" class="ui-slider-range ui-widget-header ui-corner-all ui-slider-range-min" style="left: 60%;"></div>
            <span id = "pitch-button"class="ui-slider-handle ui-state-default ui-corner-all" tabindex="0" style="left: 60%;"></span>
          </div>
          <p> Tempo </p>
          <div id = "tempo-slider" style="width:260px; margin:15px;" class="ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all">
            <div id= "tempo-level" class="ui-slider-range ui-widget-header ui-corner-all ui-slider-range-min" style="left: 60%;"></div>
            <span id = "tempo-button"class="ui-slider-handle ui-state-default ui-corner-all" tabindex="0" style="left: 60%;"></span>
          </div>

          <p> Panning - moving the sound from left to right</p>
            <input id= "panning-dial" type="text" class="dial">
          <div class= fl-r>
            <p> High pass and low pass </p>
            <div class= "nested-dials">
              <div id= "high-pass" class= "outer-dial">
                <input id= "high-pass-dial" type="text" class="dial">
              </div>
              <div id = "low-pass" class= "inner-dial">
                <input id= "low-pass-dial" type="text" class="dial">
              </div>
            </div>
          </div>

<!--         <p>Start and Stop Recording</p>
        <button onclick="startRecorder(this);">record</button>
        <button onclick="stopRecorder(this);">stop</button> -->

      </div>
    </div>
  </div>
</div>

