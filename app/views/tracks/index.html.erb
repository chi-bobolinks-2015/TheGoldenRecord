<%= render "tracks/launch" %>
<div class="container debug">
  <div class="nav nav-stacked sidebar hidden fl-l col-lg-2 p-top debug">
    <ul>
      <div class="collapsible"><%= image_tag("navigation-bar/arrow.png", height: '50', width: '50') %></div>
    <% @categories.each do |category| %>
      <li class="category-name cur fz-l f-header"><%= category.name %>
        <ul class="tracks fz-s hidden">
          <% category.tracks.each do |track| %>
            <% @track = track %>
            <li class="draggableTrack" url="<%= track.url %>" id="<%= track.id %>" image="<%= category.img %>"><a data-track-id="<%= track.id %>" data-track-title="<%= track.title %>" data-track-artist="<%= track.artist %>" data-track-description="<%= track.description %>" class="popup-window"><%= image_tag("navigation-bar/dark-plus.png") %></a><%= track.title %></li>
            <li><%= link_to "EDIT", edit_track_path(track) %></li>
          <% end %>
        </ul>
      </li>
    <% end %>
    </ul>
  </div>

  <div class="popup-info popup-box">
  </div>

  <div class="sidebar p-top collapsible col-lg-1 debug"><%= image_tag("navigation-bar/arrow-reverse.png", height: '50', width: '50') %></div>

  <div class="jquery-script-clear"></div>
  <div class="mix p-top debug fl-l col-lg-11">
    <div class="track-divs honeycombs debug">

        <div id="A" class="cell comb border debug" ><%= image_tag("sound-categories/plus.png") %><span></span></div>
        <div id="B" class="cell comb border debug" ><%= image_tag("sound-categories/plus.png") %><span></span></div>

        <div id="C" class="cell comb border debug"><%= image_tag("sound-categories/plus.png") %><span></span></div>
        <div id="D" class="cell comb border debug"><%= image_tag("sound-categories/plus.png") %><span></span></div>

        <div id="E" class="cell comb border debug"><%= image_tag("sound-categories/plus.png") %><span></span></div>
        <div id="F" class="cell comb border debug"><%= image_tag("sound-categories/plus.png") %><span></span></div>

    </div>

    <div class="deck debugs">
        <div class= "control-panel">
          <div class= "controls">
            <p> Volume </p>
            <input id= "volume-dial" type="text" class="dial">
          </div>


          <!-- <div class= "controls">
            <p> Low Pass </p>
            <input id= "low-dial" type="text" class="dial">
          </div>
           <div class= "controls">
            <p> High Pass </p>
            <input id= "high-dial" type="text" class="dial">
          </div> -->
          <div class= "controls">
            <p> Echo </p>
            <input id= "echo-dial" type="text" class="dial">
          </div>
          <div class= "controls">
            <p>Tempo </p>
            <input id= "tempo-dial" type="text" class="dial">
          </div>

          <div class= "controls">
            <p> Panning</p>
            <select id="panning-dial" class="dial">
              <option value="-1">Left</option>
              <option selected="selected" value="0">None</option>
              <option value="1">Right</option>
            </select>

          </div>




        </div>
      </div>
  </div>

      </div>
    </div>
  </div>
</div>

